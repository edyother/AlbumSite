<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=windows-1252">
    <link rel="alternate stylesheet" type="text/css" href="resource://gre-resources/plaintext.css"
      title="Wrap Long Lines">
  </head>
  <body>
    <pre>body {
  background: white none repeat scroll 0% 0%;
}

.row {
  width: 960px;
}

.page_wrap {
  margin: 25px auto;
  border-radius: 3px;
  background: white none repeat scroll 0% 0%;
}

.header_nav {
  background: white none repeat scroll 0% 0%;
  padding: 10px 0px 2px;
  margin-bottom: 25px;
box-shadow: 0px 1px 3px rgba(34, 25, 25, 0.4);
  z-index: 999;
}

.header_nav_fullwidth {
  background: #f8f8f8 none repeat scroll 0% 0%;
  padding: 10px 0px 2px;
  margin-bottom: 25px;
box-shadow: 0px 1px 3px rgba(34, 25, 25, 0.4);
}

.header_logo {
  padding-left: 10px;
  padding-top: 20px;
}

.header_logo img {
  border: medium none;
}

.blog_post {<br>
	padding-left: 20px;
	padding-right: 20px;
}

.post_meta {
  font-family: Georgia, "Times New Roman", Times, serif;
  padding-left: 20px;
	padding-right: 20px;
  font-style: italic;
  text-align: justify;
  color: #999999;
}

p {
	padding-left: 20px;
	padding-right: 20px;
  color: black;
  font-size: 13px;
}

p.welcome_text {
  font-size: 180%;
  text-align: center;
  font-weight: normal;
  font-family: Georgia, "Times New Roman", Times, serif;
  font-style: italic;
}

h1, h2, h3, h4, h5 {
  font-family: "Open Sans Condensed", sans-serif;
  font-weight: normal;
}

.heading_dots {
  background: rgba(0, 0, 0, 0) url("../images/heading_dots_grey.png") repeat-x scroll center center;
}

.heading_dots_grey {
  background: rgba(0, 0, 0, 0) url("../images/heading_dots_grey.png") repeat-x scroll center center;
}

.heading_bg {
  background: white none repeat scroll 0% 0%;
  padding: 0px 6px 0px 0px;
}

.heading_center_bg {
  background: white none repeat scroll 0% 0%;
  padding: 0px 10px;
  font-weight: lighter;
}

.heading_supersize {
  font-size: 74px;
  text-align: center;
  font-weight: normal;
  line-height: normal;
  margin: 0px 0px 10px;
  color: #999999;
}

.heading_largesize {
  font-size: 44px;
  text-align: center;
  font-weight: normal;
}

.dropcap_red, .dropcap_black {
  border-radius: 100px;
  display: block;
  float: left;
  font-size: 18px;
  line-height: 35px;
  margin: 4px 8px 0px 0px;
  text-align: center;
  width: 35px;
}

.dropcap_red {
  background: #fe563b none repeat scroll 0% 0%;
  color: white;
}

.dropcap_black {
  background: black none repeat scroll 0% 0%;
  color: white;
}

.panel {
  background: #fafafa none repeat scroll 0% 0%;
}

.panel.callout {
  background: #fe563b none repeat scroll 0% 0%;
  border: 1px solid #cccccc;
}

.panel.callout p, .panel.callout h4 {
  color: white;
}

ul.accordion &gt; li.active {
  border-top: 3px solid #f34c31;
}

dl.tabs dd.active {
  border-top: 3px solid #f34c31;
  margin-top: -3px;
}

.nav-bar {
  margin-top: 0px;
  background: #f8f8f8 none repeat scroll 0% 0%;
}

.nav-bar &gt; li {
  border: 0px none;
  padding: 0px;
  font-family: "Open Sans Condensed", sans-serif;
  letter-spacing: 1px;
box-shadow: none;
}

.nav-bar &gt; li &gt; a:first-child {
  font-size: 16px;
  text-transform: uppercase;
  padding: 0px 25px;
}

.nav-bar &gt; li:last-child {
  border: 0px none;
box-shadow: none;
}

.nav-bar &gt; li.active:hover {
  background: #f34c31 none repeat scroll 0% 0%;
  cursor: default;
}

.nav-bar &gt; li:hover {
  background: #f34c31 none repeat scroll 0% 0%;
  color: black;
transition: all 0.2s ease-in-out 0s;
}

.nav-bar &gt; li &gt; a {
  color: #4d4d4d;
}

.nav-bar &gt; li &gt; a:hover {
  color: white;
}

.active a {
  background: black none repeat scroll 0% 0%;
  color: white;
}

.nav-bar &gt; li.has-flyout &gt; a:first-child::after {
  content: "";
  display: block;
  width: 0px;
  height: 0px;
  border-width: 5px;
  border-style: solid;
  border-color: #f8f8f8 transparent transparent;
  position: absolute;
  right: 20px;
  top: 17px;
}

.nav-bar &gt; li.has-flyout &gt; a:hover:first-child::after {
  content: "";
  display: block;
  width: 0px;
  height: 0px;
  border-width: 5px;
  border-style: solid;
  border-color: #f8f8f8 transparent transparent;
  position: absolute;
  right: 20px;
  top: 17px;
}

.flyout {
  background: #f3f3f3 none repeat scroll 0% 0%;
  border-radius: 5px;
  border: 0px none;
  margin-top: -1px;
box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.7);
}

ul.flyout li, .nav-bar li ul li {
  border-left: 0px none;
}

ul.flyout li a, .nav-bar li ul li a {
  background: #f3f3f3 none repeat scroll 0% 0%;
  color: black;
  border: 0px none;
  font-size: 16px;
}

ul.flyout li a:hover, .nav-bar li ul li a:hover {
  background: #f34c31 none repeat scroll 0% 0%;
  color: white;
  border: 0px none;
transition: all 0.2s ease-in-out 0s;
}

.nav-bar.vertical &gt; li {
  border: 0px none;
}

.nav-bar.vertical &gt; li:last-child {
  border: 0px none;
}

.gallery-nav-bar {
  list-style: outside none none;
  text-align: center;
  padding-top: 10px;
  font-family: "Open Sans Condensed", sans-serif;
  letter-spacing: 2px;
  text-transform: uppercase;
}

.gallery-nav-bar li {
  display: inline-block;
  margin-right: 1px;
}

.gallery-nav-bar li {
  padding: 3px 12px;
}

.gallery-nav-bar li a {
  color: black;
  padding: 3px 12px;
}

.gallery-nav-bar li a:hover {
  background: #c02702 none repeat scroll 0% 0%;
  color: white;
  padding: 3px 12px;
  border-radius: 3px;
}

.footer_menu {
  text-align: center;
  background: rgba(0, 0, 0, 0) url("../images/heading_dots_grey.png") repeat-x scroll center top;
  margin: 25px 0px 0px;
  padding: 20px 0px;
}

.footer_menu li {
  display: inline;
  list-style: outside none none;
  margin-right: 20px;
}

.pricing_box {
  border: 1px solid #cccccc;
  float: left;
  text-align: center;
  padding: 0px 0px 20px;
  margin-top: 25px;
  margin-left: -1px;
box-shadow: 0px 2px 10px -4px rgba(0, 0, 0, 0.5);
  background: white none repeat scroll 0% 0%;
}

.pricing_box.radius-left {
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
}

.pricing_box.radius-right {
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
}

.pricing_box .header {
text-shadow: 0px 1px 1px white;
  border-bottom: 1px solid #cccccc;
  background: rgba(0, 0, 0, 0) url("../images/price.bg.png") repeat scroll 0% 0%;
  border-top-right-radius: 5px;
  border-top-left-radius: 5px;
}

.pricing_box .header {
  width: 100%;
  height: 68px;
}

.pricing_box .header span {
  position: relative;
  top: 20px;
  font-size: 16px;
  font-weight: bold;
  line-height: 28px;
}

.pricing_box .header span.info {
  position: relative;
  font-size: 12px;
  font-weight: normal;
  top: 2px;
  display: block;
  color: gray;
}

.pricing_box p {
  line-height: 28px;
}

.pricing_box ul {
  list-style: outside none none;
  margin: 0px 0px 15px !important;
  text-align: left;
}

.cols-2 .pricing_box {
  width: 49.8%;
  margin-bottom: 50px;
}

.cols-3 .pricing_box {
  width: 33%;
  margin-bottom: 50px;
}

.cols-4 .pricing_box {
  width: 24.8%;
  font-size: 1em;
}

.cols-5 .pricing_box {
  width: 19.5%;
  font-size: 0.9em;
}

.pricing_box ul li {
  line-height: 4em;
  padding-left: 20px !important;
  margin-left: 0px;
  list-style-type: none !important;
}

.pricing_box ul li.odd {
  background: #f9f9f9 none repeat scroll 0% 0%;
}

.pricing_box ul li.even {
  background: white none repeat scroll 0% 0%;
}

.pricing_box strong {
  background: rgba(0, 0, 0, 0) none no-repeat scroll 0px -9999px;
  height: 12px;
  padding-left: 20px;
  font-style: normal;
  color: gray;
}

.pricing_box strong.no {
  background-position: 0px 0px;
}

.pricing_box strong.yes {
  background-position: 0px -15px;
}

.pricing_box strong.single {
  background-position: 0px -30px;
}

.pricing_box strong.unlim {
  background-position: 0px -45px;
}

.pricing_box strong.more {
  background: rgba(0, 0, 0, 0) none no-repeat scroll 0px 0px;
  height: 20px;
  width: 20px;
  cursor: pointer;
  float: right;
  padding: 0px;
  margin: 14px 14px 0px 0px;
}

.pricing_box strong.nopad {
  padding: 0px;
}

.pricing_box h3 {
  margin: 20px 0px;
  font-size: 22px;
  display: block;
}

.pricing_box h3 em {
  font-size: 12px;
  font-weight: normal;
}

.pricing_box p.button {
  height: 40px;
  background: rgba(0, 0, 0, 0) none no-repeat scroll left 0px;
  float: none;
}

.pricing_box p.button a {
  font-size: 16px;
  font-weight: bold;
  line-height: 40px;
  background: rgba(0, 0, 0, 0) none no-repeat scroll right -40px;
  padding: 0px 40px;
}

.pricing_box p.button:hover {
  background-position: left -80px;
}

.pricing_box p.button a:hover {
  background-position: right -120px;
}

.pricing_box.large {
  position: relative;
  z-index: 9;
  margin-top: 0px;
box-shadow: 0px 1px 10px #cbcbcb;
  padding: 0px 0px 40px;
  border-radius: 5px;
}

.pricing_box.large .header {
  color: #303e0e;
text-shadow: 0px 1px 0px #c7dd90;
  height: 88px;
  background: rgba(0, 0, 0, 0) url("../images/price.bg.signup.png") repeat scroll 0% 0%;
  border-bottom: 1px solid #b2d850;
}

.pricing_box.large .header span {
  top: 30px;
}

.pricing_box.large .header span.info {
  top: 14px;
  color: #576e1f;
text-shadow: none;
}

.pricing_box.large p.button {
  background-position: left -160px;
}

.pricing_box.large p.button a {
  background-position: right -200px;
  color: #303e0e;
text-shadow: 0px 1px 0px #c7dd90;
}

.pricing_box.large p.button:hover {
  background-position: left -240px;
}

.pricing_box.large p.button a:hover {
  background-position: right -280px;
}

.centered {
  margin: 0px auto;
}

.box {
  margin: 5px;
  padding: 10px;
  background: white none repeat scroll 0% 0%;
  border: 1px solid #cccccc;
box-shadow: 0px 1px 3px rgba(34, 25, 25, 0.4);
  font-size: 100%;
  line-height: 1.4em;
  float: left;
}

.box p {
  font-size: 12px;
  margin: 0px;
  padding: 12px 0px;
}

.box h2 {
  font-size: 14px;
  font-weight: 200;
}

.box h4 {
  margin-bottom: 0px;
  font-size: 14px;
  font-family: Georgia, "Times New Roman", Times, serif;
  padding: 0px;
}

.box img, #tumblelog img {
  display: block;
  width: 100%;
}

.box_comment {
  background: #f2f0f0 none repeat scroll 0% 0%;
  padding: 8px;
  border-bottom: 1px solid #d9d4d4;
  border-top: 1px solid #fcfafa;
  overflow: hidden;
}

.box_comment p {
  font-size: 11px;
  padding: 0px;
  margin: 0px;
}

.rtl .box {
  float: right;
  text-align: right;
  direction: rtl;
}

.col1 {
  width: 80px;
}

.col2 {
  width: 222px;
}

.col3 {
  width: 280px;
}

.col4 {
  width: 380px;
}

.col5 {
  width: 480px;
}

.col1 img {
  max-width: 80px;
}

.col2 img {
  max-width: 211px;
}

.col3 img {
  max-width: 280px;
}

.col4 img {
  max-width: 380px;
}

.col5 img {
  max-width: 480px;
}

.box_fluid {
  margin-bottom: 1.4%;
  padding: 1px;
  background: white none repeat scroll 0% 0%;
  border: 1px solid #ededed;
  font-size: 100%;
  line-height: 1em;
  float: left;
}

.box_fluid_inner {
  padding: 0px 15px;
}

.col_fluid {
  width: 50%;
}

.col_fluid img {
  width: 100%;
}

</pre>
  </body>
</html>
